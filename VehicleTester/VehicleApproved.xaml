<Window x:Class="VehicleTester.VehicleApproved"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VehicleTester"
        mc:Ignorable="d"
        Title="VehicleApproved" Height="400" Width="600">
    <Grid Background="#FF363131">
        <Border BorderBrush="Black" BorderThickness="15" Height="370" VerticalAlignment="Top"/>
        <Button x:Name="RestartQuestionaire" Content="Restart Questionaire" Background="Black" Foreground="White" HorizontalAlignment="Left" Margin="216,264,0,0" VerticalAlignment="Top" Width="143" Click="RestartQuestionaire_Click"/>
        <Button x:Name="Back" Content="Back" HorizontalAlignment="Left" Background="Black" Foreground="White" Margin="23,24,0,0" VerticalAlignment="Top" Width="75" Click="Back_Click"/>
        <Label x:Name="Congratulations" Content="Congratulations!!!" HorizontalAlignment="Left" Margin="185,78,0,0"  Foreground="White" VerticalAlignment="Top" Width="198" FontSize="24"/>
        <Label x:Name="Passed"  HorizontalAlignment="Left" Margin="141,125,0,0" Foreground="White" VerticalAlignment="Top" Width="292" FontSize="18.667" Height="42"/>
    </Grid>
</Window>
